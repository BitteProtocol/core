{
  "name": "@bitte/core",
  "version": "1.0.0",
  "private": true,
  "packageManager": "bun@1.1.37",
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "build": "bun run --cwd packages/agent-sdk build",
    "tokenMap": "bun run scripts/genTokenMap.ts",
    "lint": "prettier --check {scripts,packages}/**/* && eslint packages/",
    "fmt": "prettier --write {scripts,packages}/**/* && eslint packages/ --fix",
    "test": "jest"
  },
  "devDependencies": {
    "@duneanalytics/client-sdk": "^0.2.5",
    "@types/bun": "latest",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.26.0",
    "jest": "^29.7.0",
    "next": "^15.3.1",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "viem": "^2.29.0"
  }
}
