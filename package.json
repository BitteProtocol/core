{
  "name": "@bitte/core",
  "version": "1.0.0",
  "private": true,
  "packageManager": "bun@1.1.37",
  "workspaces": [
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "build": "bun run --cwd packages/agent-sdk build",
    "tokenMap": "bun run scripts/genTokenMap.ts",
