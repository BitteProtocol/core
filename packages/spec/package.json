{
  "name": "@bitte-ai/spec",
  "version": "0.0.13",
  "author": "bitteprotocol",
  "description": "Spec package for Bitte Protocol including types and validation utils",
  "keywords": [
    "bitte-ai",
    "bitte",
    "openapi",
    "types",
    "validation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitteprotocol/core.git"
  },
  "bugs": {
    "url": "https://github.com/bitteprotocol/core/issues"
  },
  "homepage": "https://bitte.ai",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -fr dist/* && bun build:esm && bun build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json"
  },
  "dependencies": {
    "@scalar/openapi-parser": "^0.10.6",
    "ajv": "^8.12.0",
    "ajv-draft-04": "^1.0.0",
    "openapi-types": "^12.1.3",
    "zod": "^3.24.2"
  },
  "license": "MIT"
}