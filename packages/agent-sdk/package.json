{
  "name": "@bitte-ai/agent-sdk",
  "version": "0.0.2",
  "author": "bh2smith",
  "description": "Agent SDK for Bitte Protocol",
  "keywords": [
    "bitte",
    "protocol",
    "agent",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitteprotocol/core.git"
  },
  "bugs": {
    "url": "https://github.com/bitteprotocol/core/issues"
  },
  "homepage": "https://bitte.ai",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./near": {
      "types": "./dist/esm/near/index.d.ts",
      "import": "./dist/esm/near/index.js",
      "require": "./dist/cjs/near/index.js"
    },
    "./evm": {
      "types": "./dist/esm/evm/index.d.ts",
      "import": "./dist/esm/evm/index.js",
      "require": "./dist/cjs/evm/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -fr dist/* && bun build:esm && bun build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json"
  },
  "dependencies": {
    "@bitte-ai/types": "^0.10.0",
    "near-api-js": "^6.3.0",
    "viem": "^2.37.5",
    "zerion-sdk": "^0.1.6"
  }
}
