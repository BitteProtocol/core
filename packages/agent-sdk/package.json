{
  "name": "@bitte-ai/agent-sdk",
  "version": "0.0.2",
  "author": "bh2smith",
  "description": "Agent SDK for Bitte Protocol",
  "keywords": [
    "bitte",
    "protocol",
    "agent",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitteprotocol/core.git"
  },
  "bugs": {
    "url": "https://github.com/bitteprotocol/core/issues"
  },
  "homepage": "https://bitte.ai",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -fr dist/* && bun build:esm && bun build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json"
  },
  "dependencies": {
    "@bitte-ai/types": "^0.5.3",
    "near-safe": "^0.10.0",
    "viem": "^2.31.6",
    "zerion-sdk": "^0.1.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
