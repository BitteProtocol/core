{
  "name": "@bitte-ai/agent-sdk",
  "version": "0.0.2",
  "author": "bh2smith",
  "description": "Agent SDK for Bitte Protocol",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "tests/dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist && rm -rf tests/dist",
    "lint": "eslint \"./**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "test": "vitest run"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tests": {
      "types": "./tests/dist/index.d.ts",
      "import": "./tests/dist/index.mjs",
      "module": "./tests/dist/index.mjs",
      "require": "./tests/dist/index.js"
    }
  },
  "dependencies": {
    "viem": "^2.23.4",
    "near-safe": "^0.9.11",
    "zerion-sdk": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "tsup": "^8.3.6",
    "@bitte/ai-tsconfig": "workspace:*",
    "typescript": "^5.7.3",
    "zod": "^3.24.2"
  },
  "keywords": [
    "bitte",
    "protocol",
    "agent",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitteprotocol/core.git"
  },
  "bugs": {
    "url": "https://github.com/bitteprotocol/core/issues"
  },
  "homepage": "https://bitte.ai",
  "publishConfig": {
    "access": "public"
  }
}